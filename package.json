{
  "name": "whole-new-world",
  "version": "1.0.0",
  "description": "A repo to help me practice version control and full stack JS",
  "main": "index.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --progress -p",
    "dev": "cross-env NODE_ENV=development webpack --watch --progress",
    "server": "node ./dist/server.js",
    "devServer": "cross-env NODE_ENV=development nodemon ./dist/server.js",
    "start": "npm run prod && npm run server",
    "startDev": "concurrently \"npm run dev\" \"npm run devServer\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finbarmaginn/whole-new-world.git"
  },
